{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "username": "bobbyJo",
      "first_inital": "B",
      "last_name": "Jo",
      "password": "bobbyJo",
      "status": "unactive"
    }
  ],
  "required": ["username", "first_inital", "last_name", "password", "status"],
  "properties": {
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The username schema",
      "description": "username must be unique",
      "default": "",
      "examples": ["bobbyJo"]
    },
    "first_inital": {
      "$id": "#/properties/first_inital",
      "type": "string",
      "title": "The first_inital schema",
      "description": "first inital of first name",
      "default": "",
      "examples": ["B"]
    },
    "last_name": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "The last_name schema",
      "description": "last name",
      "default": "",
      "examples": ["Jo"]
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The password schema",
      "description": "password, admin can change to anything",
      "default": "",
      "examples": ["bobbyJo"]
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The status schema",
      "description": "work status {unactive, employee, manager, admin",
      "default": "",
      "examples": ["unactive"]
    }
  },
  "additionalProperties": true
}
